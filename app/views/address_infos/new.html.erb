<%= render 'layouts/errors', obj: @address %>
<h1 class="h4 text-gray-900 mb-4"><%= t('address_infos.title') %></h1>
<%= form_with scope: :address, url: create_address_infos_path, local: true, html: { class: 'user' } do |f| %>
  <%= render 'address_infos/form', obj: f %>
  <div class="form-group row">
      <div class="col-sm-12 ">
          <%= link_to t('common.back'), new_personal_info_path, class: 'btn btn-secondary btn-user' %>
          <%= f.submit t('common.next'), class: 'btn btn-secondary float-right'  %>
      </div>
  </div>
<% end %>
